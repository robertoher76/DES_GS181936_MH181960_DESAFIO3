  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT 
ta.Descripcion as 'Tarea',
pr.Nombre as 'Proyecto',
et.Nombre
FROM 
Tareas ta 
Inner Join Proyectos pr on pr.Id = ta.ProyectoId
Inner Join Equipos eq on eq.Id = pr.EquipoId
Inner Join EstadoTareas et on et.Id = ta.EstadoTareasId
ORDER BY et.Nombre</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Tarea">
          <DataField>Tarea</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Proyecto">
          <DataField>Proyecto</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Nombre">
          <DataField>Nombre</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
